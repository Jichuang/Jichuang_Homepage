<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="org.hope6537.model.Userinfo" table="Userinfo_DataBase">
		<id name="uid" type="integer">
			<column name="uid"></column>
			<generator class="increment"></generator>
		</id>
		<property name="uname" type="string">
			<column name="uname"></column>
		</property>
		<property name="upassword" type="string">
			<column name="upassword"></column>
		</property>
		<property name="uusername" type="string">
			<column name="uusername"></column>
		</property>
		<property name="ustatus" type="string">
			<column name="ustatus"></column>
		</property>
		<property name="uimages" type="string"> 
			<column name="uimages"></column>
		</property>
	</class>

</hibernate-mapping>
